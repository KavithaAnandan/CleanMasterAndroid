<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cleanmaster">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splashscreen.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".appmodules.welcomescreen.WelcomeScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name=".appmodules.welcomescreen.SigninScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name=".appmodules.homescreen.HomeScreen"
            android:screenOrientation="portrait" />
    </application>
</manifest>